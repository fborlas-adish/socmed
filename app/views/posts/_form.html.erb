<%= form_with model: user_post do |form| %>
  <div class="mb-3">
    <%= form.label :body, class: "form-label" %><br>
    <%= form.text_area :body, class: "form-control" %>
  </div>

  <div class="mb-3">
    <%= form.file_field :media, multiple: true %> 
  </div>

  <div>
    <%= form.submit "Submit", class: "btn btn-outline-primary" %>
  </div>
<% end %>